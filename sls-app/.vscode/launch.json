  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug serverless app",
      "program": "${workspaceFolder}/node_modules/serverless/bin/serverless",
      "args": [
        "offline",
        "--httpPort",
        "3333",
        "--noTimeout"
      ],
      "runtimeExecutable": "node",
      "windows": {
        "program": "${workspaceFolder}\\node_modules\\serverless\\bin\\serverless",
      },
    }
  ]
}